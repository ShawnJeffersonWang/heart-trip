syntax = "v1"

info(
    title: "聊天服务"
    desc: "聊天服务"
    author: "Shawn"
    email: "xw2002121@gmail.com"
    version: "v1"
)

import (
    "websock/ws.api"
)

//need login
@server(
    prefix: websocket/v1
    group: websock
    jwt: JwtAuth
)
service websocket {
    @doc "chat"
    @handler chat
    get /chat

    @doc "get inbox messages"
    @handler getInbox
    get /getInbox (GetInboxReq) returns (GetInboxResp)
}

